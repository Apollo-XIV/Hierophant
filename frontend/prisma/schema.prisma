generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "mongodb"
  url      = "http://db:27017/"
}

model posts {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  abstract String
  slug     String @unique
  title    String
  url      String
}
