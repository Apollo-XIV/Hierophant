// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_DB_URL")
}
//test

model posts {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  abstract String
  slug     String @unique
  title    String
  url      String
}
